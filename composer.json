{
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Codeception/Codeception"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "codeception/codeception": "2.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
    },
    "config": {
        "preferred-install": "dist"
    }
}
